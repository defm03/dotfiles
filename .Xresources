!- pseudo transparency
urxvt*shading:                        10
!urxvt*transparent:                   true

iURxvt.iconFile: /usr/share/icons/Faenza/apps/scalable/terminal.svg
URxvt.perl-ext-common:                vtwheel
URxvt*.scrollBar:                     false
